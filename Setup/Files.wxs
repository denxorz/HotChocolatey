<?xml version="1.0" encoding="utf-8"?>
<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi">
    <Fragment>
        <DirectoryRef Id="INSTALLFOLDER">
            <Component Id="HotChocolatey.exe" Guid="*">
                <File Id="HotChocolatey.exe" KeyPath="yes" Source="$(var.HotChocolatey.TargetDir)\HotChocolatey.exe" />
            </Component>
            <Component Id="HotChocolatey.exe.config" Guid="*">
                <File Id="HotChocolatey.exe.config" KeyPath="yes" Source="$(var.HotChocolatey.TargetDir)\HotChocolatey.exe.config" />
            </Component>
            
            
            
            
            <Component Id="HtmlRenderer.dll" Guid="*">
                <File Id="HtmlRenderer.dll" KeyPath="yes" Source="$(var.HotChocolatey.TargetDir)\HtmlRenderer.dll" />
            </Component>
            
            <Component Id="HtmlRenderer.WPF.dll" Guid="*">
                <File Id="HtmlRenderer.WPF.dll" KeyPath="yes" Source="$(var.HotChocolatey.TargetDir)\HtmlRenderer.WPF.dll" />
            </Component>
            
            <Component Id="log4net.dll" Guid="*">
                <File Id="log4net.dll" KeyPath="yes" Source="$(var.HotChocolatey.TargetDir)\log4net.dll" />
            </Component>
            
            <Component Id="MarkdownSharp.dll" Guid="*">
                <File Id="MarkdownSharp.dll" KeyPath="yes" Source="$(var.HotChocolatey.TargetDir)\MarkdownSharp.dll" />
            </Component>
            
            
            <Component Id="Microsoft.Web.XmlTransform.dll" Guid="*">
                <File Id="Microsoft.Web.XmlTransform.dll" KeyPath="yes" Source="$(var.HotChocolatey.TargetDir)\Microsoft.Web.XmlTransform.dll" />
            </Component>
            <Component Id="Newtonsoft.Json.dll" Guid="*">
                <File Id="Newtonsoft.Json.dll" KeyPath="yes" Source="$(var.HotChocolatey.TargetDir)\Newtonsoft.Json.dll" />
            </Component>
            
            <Component Id="NuGet.Core.dll" Guid="*">
                <File Id="NuGet.Core.dll" KeyPath="yes" Source="$(var.HotChocolatey.TargetDir)\NuGet.Core.dll" />
            </Component>
            <Component Id="System.Net.Http.Extensions.dll" Guid="*">
                <File Id="System.Net.Http.Extensions.dll" KeyPath="yes" Source="$(var.HotChocolatey.TargetDir)\System.Net.Http.Extensions.dll" />
            </Component>
            
            <Component Id="System.Net.Http.Primitives.dll" Guid="*">
                <File Id="System.Net.Http.Primitives.dll" KeyPath="yes" Source="$(var.HotChocolatey.TargetDir)\System.Net.Http.Primitives.dll" />
            </Component>
            
        </DirectoryRef>
    </Fragment>
    <Fragment>
        <ComponentGroup Id="ProductComponents">
            <ComponentRef Id="HotChocolatey.exe" />
            <ComponentRef Id="HotChocolatey.exe.config" />
            
            
            
            
            <ComponentRef Id="HtmlRenderer.dll" />
            
            <ComponentRef Id="HtmlRenderer.WPF.dll" />
            
            <ComponentRef Id="log4net.dll" />
            
            <ComponentRef Id="MarkdownSharp.dll" />
            
            
            <ComponentRef Id="Microsoft.Web.XmlTransform.dll" />
            <ComponentRef Id="Newtonsoft.Json.dll" />
            
            <ComponentRef Id="NuGet.Core.dll" />
            <ComponentRef Id="System.Net.Http.Extensions.dll" />
            
            <ComponentRef Id="System.Net.Http.Primitives.dll" />
            
        </ComponentGroup>
    </Fragment>
</Wix>